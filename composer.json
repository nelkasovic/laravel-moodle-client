{
  "name": "wimando/laravel-moodle",
  "description": "Laravel Moodle Client",
  "license": "MIT",
  "authors": [
    {
      "name": "Nermin Elkasovic",
      "email": "nermin@wimando.ch",
      "homepage": "https://wimando.ch"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "guzzlehttp/guzzle": "^7.0",
    "beberlei/assert": "^3.1",
    "ext-soap": "*",
    "ext-simplexml": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0",
    "laravel/framework": "^8.50"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Wimando\\LaravelMoodle\\LaravelMoodleServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Wimando\\LaravelMoodle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wimando\\LaravelMoodle\\Tests\\": "tests/"
    }
  }
}
